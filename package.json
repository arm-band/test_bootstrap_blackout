{
    "name": "mori",
    "version": "0.3.0",
    "description": "Gulpを使用した最低限のフレームワークです。",
    "main": "gulpfile.js",
    "scripts": {
        "gulp": "gulp",
        "start": "npm-run-all -s itallcomesbacktome gulp",
        "itallcomesbacktome": "npm-run-all -s memory black",
        "memory": "mkdirp src/scss/assets",
        "black": "npm-run-all -p momentarily:black immomentarily:black so:black tearinme holeinme blackinme",
        "momentarily:black": "echo \"It all comes back to me.\"",
        "immomentarily:black": "cpx \"./node_modules/bootstrap-honoka/scss/*.scss\" \"./src/scss/assets/bootstrap\"",
        "so:black": "node ./gulp/soblack",
        "tearinme": "cpx \"./node_modules/bootstrap-honoka/scss/honoka/*.scss\" \"./src/scss/assets/bootstrap/honoka/honoka\"",
        "holeinme": "cpx \"./node_modules/bootstrap/scss/**/*.scss\" \"./src/scss/assets/bootstrap/honoka/bootstrap/scss\"",
        "blackinme": "echo \"Black!\""
    },
    "author": "Arm Band",
    "license": "ISC",
    "dependencies": {
        "bootstrap": "^4.5.3",
        "bootstrap-honoka": "^4.4.0"
    },
    "devDependencies": {
        "browser-sync": "^2.26.13",
        "cpx": "^1.5.0",
        "gulp": "^4.0.2",
        "gulp-concat": "^2.6.1",
        "gulp-data": "^1.3.1",
        "gulp-ejs": "^5.1.0",
        "gulp-notify": "^3.2.0",
        "gulp-plumber": "^1.2.1",
        "gulp-rename": "^2.0.0",
        "gulp-sass": "^4.1.0",
        "sass": "^1.29.0",
        "fibers": "^5.0.0",
        "gulp-autoprefixer": "^7.0.1",
        "gulp-uglify-es": "^2.0.0",
        "mkdirp": "^1.0.4",
        "npm-run-all": "^4.1.5"
    },
    "engines": {
        "node": ">=14.0.0",
        "npm": ">=6.14.4"
    },
    "engineStrict": true,
    "prettier": {}
}
